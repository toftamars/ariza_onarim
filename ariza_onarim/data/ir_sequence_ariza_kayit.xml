<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_ariza_kayit" model="ir.sequence">
            <field name="name">Arıza Kayıt Numarası</field>
            <field name="code">ariza.kayit</field>
            <field name="prefix">ARZ-%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_ariza_kayit_barcode" model="ir.sequence">
            <field name="name">Arıza Kayıt Barkod (Müşteri Ürünü)</field>
            <field name="code">ariza.kayit.barcode</field>
            <field name="prefix">BC-%(year)s-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 